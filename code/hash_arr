#include <string>
#include <vector>

struct hash_arr {
    long long P = 257, MOD = 1e9 + 9;
    vector <long long> h, p;
    hash_arr(string s) {
        int len = s.length() + 1;
        p.resize(len);
        h.resize(len);
        p[0] = 1;
        for(int i = 1; i < len; i++) {
            p[i] = p[i - 1] * P % MOD;
            h[i] = (h[i - 1] * P + s[i - 1] + 1) % MOD;
        }
    }
    long long sb(int l, int r) {
        return ((h[r] - h[l] * (p[r - l])) % MOD + MOD) % MOD;
    }
};
